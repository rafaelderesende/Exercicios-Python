
cont_div = 0 

numero = int(input('Digite um número: '))
for c in range(1, numero + 1):
    if numero % c == 0:
        print('\033[33m', end= ' ')
        cont_div = cont_div + 1
    else:
        print('\033[31m', end=' ')
        print('{}'.format(c), end=' ')
print('\n\033[m O número {} foi divisível {} vezes'.format(numero, cont_div), end=' ')
if cont_div == 2:
    print('Este número é PRIMO!')
else:
    print('Este Número não é PRIMO!')


        


