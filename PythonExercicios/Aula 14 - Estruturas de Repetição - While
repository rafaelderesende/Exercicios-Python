c = 1
while c < 10:
    print(c)
    c = c + 1
print('fim!')

n = 1
while n != 0:
    n = int(input('Digite um número: '))
print('Fim!')

resposta = 'S'
while resposta == 'S':
    numero = int(input('Digite um número: '))
    resposta = str(input('Deseja Continuar [S/N]: ')).upper()
print('Fim!')